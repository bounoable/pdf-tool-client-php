{
    "name": "bounoable/pdf-tool-client-php",
    "description": "PHP client for pdf-tool-server.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Saman Hosseini",
            "email": "bounoable@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "^5.8|^6.0"
    },
    "autoload": {
        "psr-4": {
            "PdfTools\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PdfTools\\Laravel\\ServiceProvider"
            ],
            "aliases": {
                "PdfTools": "PdfTools\\Laravel\\Facade"
            }
        }
    }
}
